        <div class="data-table-panel panel panel-default">
            <div class="panel-heading">表格名称</div>
            <div class="panel-body"></div>
        </div>
        <!-- end 数据表格展示面板 -->

        <!-- 数据图表展示面板 -->
        <div class="data-canvas-panel panel panel-default">
            <div class="panel-heading">图表名称</div>
            <div class="panel-body">
                <div class="data-canvas-container">
                    <div id="canvasjs" style="width:400px;height:360px;"></div>
                </div>
            </div>
        </div>
        <!-- end 数据图表展示面板 -->

    </div>
