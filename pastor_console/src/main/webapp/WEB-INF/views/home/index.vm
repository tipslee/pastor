<div class="explore-container">
    <div class="panel-container clearfix">

        <!-- 数据编辑工具面板 -->
        <div class="edit-tool-panel panel panel-default">
            <div class="panel-body clearfix">
                <div class="pull-right clearfix">
                    <ul class="clearfix pull-left">
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-sort">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">排序</span>
                            </a>
                        </li>
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-filter">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">筛选</span>
                            </a>
                        </li>
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-insert-column">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">插入列</span>
                            </a>
                        </li>
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-del-column">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">删除列</span>
                            </a>
                        </li>
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-customizations">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">自定义列</span>
                            </a>
                        </li>
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-function">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">函数</span>
                            </a>
                        </li>
                    </ul>
                    <ul class="clearfix pull-left">
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-format-num">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">数字格式</span>
                            </a>
                        </li>
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-format-requirement">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">条件格式</span>
                            </a>
                        </li>
                    </ul>
                    <ul class="clearfix pull-left">
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-insert-canvas">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">插入图表</span>
                            </a>
                        </li>
                        <li class="pull-left">
                            <a href="javascript:void(0);" class="edit-tool-canvas-color">
                                <span class="edit-tool-icon"></span>
                                <span class="tool-name">图表颜色</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- end 数据编辑工具面板 -->

