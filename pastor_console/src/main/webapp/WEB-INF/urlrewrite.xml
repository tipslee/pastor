<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>

    <rule>
        <from>^/show/([\d]*).htm</from>
        <to>/show?id=$1</to>
    </rule>
    <!--首页 需要放到最后-->
    <rule>
        <from>^/index.htm</from>
        <to>/</to>
    </rule>

</urlrewrite>
